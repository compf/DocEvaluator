{
  "name": "doc_evaluator",
  "version": "3.0.0",
  "description": "A tool to evaluate the documentation of source code",
  "type": "commonjs",
  "scripts": {
    "launch": "tsc && node build/src/index.js",
    "test": "tsc && jest",
    "deploy": "npm i && npm link typescript && ncc build src/index.ts -o build",
    "build": "tsc",
    "prepack": "npm run build"
  },
  "keywords": [
    "documentation",
    "evaluation",
    "comments",
    "comment metrics"
  ],
  "bin": "./build/src/index.js",
  "main": "./build/src/index.js",
  "author": "Timo Schoemaker",
  "license": "MIT",
  "esModuleInteropt": true,
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-export-default-from": "^7.14.5",
    "@babel/preset-env": "^7.15.8",
    "@types/antlr4": "^4.7.2",
    "@types/levenshtein": "^1.0.1",
    "@types/minimatch": "^3.0.5",
    "@types/node": "^16.11.1",
    "@types/spellchecker": "^3.5.0",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "babel-jest": "^27.3.0",
    "jest": "^27.3.1"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.2",
    "antlr4": "^4.8.0",
    "antlr4ts": "^0.5.0-alpha.4",
    "chalk": "^4.1.2",
    "compromise": "^13.11.4",
    "compromise-syllables": "^0.0.6",
    "levenshtein": "^1.0.5",
    "minimatch": "^3.0.4",
    "spellchecker": "^3.7.1"
  }
}
